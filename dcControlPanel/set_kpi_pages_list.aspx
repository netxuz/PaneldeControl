<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="set_kpi_pages_list.aspx.cs" Inherits="dcControlPanel.set_kpi_pages" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
  <title>Panel de Control DebtControl</title>
  <link href="style/screen.css" type="text/css" rel="stylesheet" />
</head>
<body>
  <form id="form1" runat="server">
    <table border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
      <tr>
        <td style="height: 50px; text-align: center; vertical-align: central;">
          <label class="titMantenedor">Asignación de KPI a Páginas</label>
        </td>
      </tr>
      <tr>
        <td style="background-color: #00619F; height: 5px;"></td>
      </tr>
      <tr>
        <td>
          <asp:Button ID="btnVolver" runat="server" Text="Volver" OnClick="btnVolver_Click" />
        </td>
      </tr>
      <tr>
        <td>
          <asp:GridView ID="gridKpiPage" runat="server" CssClass="tbVistaMonitor" BorderStyle="None"
            DataKeyNames="cod_page" BorderWidth="0" GridLines="None" OnSelectedIndexChanged="gridKpiPage_SelectedIndexChanged"
            AutoGenerateDeleteButton="true"
            AutoGenerateSelectButton="true"
            AutoGenerateColumns="false">
            <Columns>
              <asp:BoundField ControlStyle-CssClass="tbTitRow" HeaderText="Nombre Pagina" DataField="nom_page">
                <HeaderStyle HorizontalAlign="Center" />
                <ItemStyle HorizontalAlign="Left" CssClass="tbTitRow" />
              </asp:BoundField>
            </Columns>
          </asp:GridView>
        </td>
      </tr>
    </table>
    <asp:HiddenField ID="CodCliente" runat="server" />
  </form>
</body>
</html>
